/**
 * 4-way Or gate: 
 * out = in[0] Or in[1] Or ... Or in[3]
 */
CHIP Or4Way {
    IN in[4];
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Or(a= in[0], b= in[1], out= o1);
    Or(a= in[2], b= in[3], out= o2);
    Or(a= o1, b= o2, out= out);
}