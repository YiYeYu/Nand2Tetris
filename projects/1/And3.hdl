/**
 * And gate:
 * if (a and b and c) out = 1, else out = 0 
 */
CHIP And3 {
    IN a, b, c;
    OUT out;
    
    PARTS:
    //// Replace this comment with your code.
    And(a= a, b= b, out= d);
    And(a= d, b= c, out= out);
}