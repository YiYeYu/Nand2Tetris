/**
 * Or gate:
 * if (a or b or c) out = 1, else out = 0 
 */
CHIP Or3 {
    IN a, b, c;
    OUT out;
    
    PARTS:
    //// Replace this comment with your code.
    Or(a= a, b= b, out= d);
    Or(a= d, b= c, out= out);
}