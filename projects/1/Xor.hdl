/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in= a, out= na);
    Not(in= b, out= nb);
    And(a= a, b= nb, out= f1);
    And(a= na, b= b, out= f2);
    Or(a= f1, b= f2, out= out);
}